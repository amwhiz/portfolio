{
  "name": "ntc-in-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --cache src packages --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --config .prettierrc src/**/**.ts packages/**/**.ts --check",
    "format:fix": "prettier --config .prettierrc src/**/**.ts packages/**/**.ts -write",
    "start:dev": "sls offline --aws-profile ntc --stage dev",
    "start:prd": "sls offline --aws-profile ntc --stage prd",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@aw/cds": "^1.0.0",
    "@aw/crm": "^1.0.0",
    "@aw/dynamoDB": "^1.0.0",
    "@aw/libs": "^1.0.0",
    "@aw/logger": "^1.0.0",
    "@aw/mail": "^1.0.0",
    "@aw/objectmapper": "^1.0.0",
    "@aw/pg": "^1.0.0",
    "@aw/s3": "^1.0.0",
    "@aw/scheduler": "^1.0.0",
    "@aw/shipment": "^1.0.0",
    "@aw/wati": "^1.0.0",
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "@mikro-orm/core": "^5.9.7",
    "@mikro-orm/postgresql": "^5.9.7",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "http-status-codes": "^2.3.0",
    "json-schema-to-ts": "^2.9.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "string-template": "^1.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.9.5",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^14.14.25",
    "@types/string-template": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "prettier": "3.1.1",
    "serverless": "^3.0.0",
    "serverless-api-gateway-caching": "^1.10.3",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^13.3.1",
    "serverless-plugin-log-retention": "^2.0.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikroOrmConfig.ts"
    ]
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
